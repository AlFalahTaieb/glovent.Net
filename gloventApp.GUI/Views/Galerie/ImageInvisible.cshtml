@model IEnumerable<gloventApp.GUI.Models.imageModel>

@{
    ViewBag.Title = "ImageInvisible";
}

<h2>ImageInvisible</h2>

<

<div class="box">
    <div class="box-header">
        <h3 class="box-title">Hover Data Table</h3>
    </div>
    <!-- /.box-header -->
    <div class="box-body">
        <table id="example2" class="table table-bordered table-hover">
            <thead>
            <tbody>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.aime)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.danger)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.visib)
                    </th>

                    <th>
                        @Html.DisplayNameFor(model => model.dateinvisibl)
                    </th>

                    <th>Action</th>

                </tr>
            </tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.aime)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.danger)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.visib)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.dateinvisibl)
                    </td>

                    <td>

                        @Html.ActionLink("Details", "DetailsImage", new { id = item.id })
                    </td>
                    <td>

                        @Html.ActionLink("Delete", "DeleteImage", new { id = item.id })
                    </td>

                </tr>
            }

        </table>
    </div>
</div>