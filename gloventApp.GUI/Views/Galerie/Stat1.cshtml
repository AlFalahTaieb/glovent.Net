
@model IEnumerable<gloventApp.GUI.Models.galerieModel>

@{
    ViewBag.Title = "StatReunion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script>
 var data =JSON.parse('@ViewBag.Data');

    </script>


<link rel="stylesheet" href="@Url.Content("~/Content/css/nexus.css")" rel="stylesheet">

<ul class="portfolio-group">
    <h2>StatReunion</h2>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load("current", {packages:["corechart"]});
      google.charts.setOnLoadCallback(drawChart);
      function drawChart() {
        var data = google.visualization.arrayToDataTable(data);

        var options = {
            title: 'Reunion and Type',
            is3D: true,

        };

        var chart = new google.visualization.PieChart(document.getElementById('piechart_3d'));
        chart.draw(data, options);
      }
    </script>


    <div id="piechart_3d" style="width: 900px; height: 500px;"></div>